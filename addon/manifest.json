{
  "name": "TYPO3 TDK GitPod",
  "manifest_version": 2,
  "description": "Run TYPO3 TDK with current patch in GitPod",
  "short_name": "TYPO3 TDK integration",
  "version": "1.0.0",
  "default_locale": "en",
  "homepage_url": "https://github.com/ochorocho/tdk-gitpod-addon",
  "icons": {
    "48": "icons/typo3-orange.svg",
    "96": "icons/typo3-orange.svg"
  },
  "permissions": [
	"*://review.typo3.org/*",
    "activeTab"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "browser_action": {
    "default_icon": "icons/typo3-dark.svg",
    "theme_icons": [{
      "light": "icons/typo3-light.svg",
      "dark": "icons/typo3-dark.svg",
      "size": 48
    }],
    "default_title": "TYPO3 TDK",
	"default_popup": "popup.html"
  }
}
